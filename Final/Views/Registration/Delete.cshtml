@model Final.Models.Registration

<h2>Delete</h2>

<h3>Are you sure you want to delete: @Model.RegistrationId</h3>

@using (Html.BeginForm("ConfirmDelete", "Registration", FormMethod.Post)) {
	<input type="hidden" asp-for="RegistrationId" />
	<button asp-action="Index">Back</button>
	<button class="btn btn-danger" type="submit">Delete</button>
}